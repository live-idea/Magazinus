<h1>New good</h1>

<% form_for(@good, :url=>category_goods_path(:category_id=>params[:category_id]), :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <p>
    <%= f.submit 'Create' %>
  </p>


<% end %>

<%= link_to 'Back',category_goods_path(:category_id=>params[:category_id]) %>

